{
    "version": 2,
    "functions": {
        "src/index.js": {
            "maxDuration": 300
        },
        "api/**/*.js": {
            "maxDuration": 60
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/src/index.js"
        }
    ],
    "env": {
        "OCR_IMPLEMENTATION": "External",
        "PADDLEOCR_SERVICE_URL": "@paddleocr_service_url",
        "PADDLEOCR_API_KEY": "@paddleocr_api_key"
    }
}