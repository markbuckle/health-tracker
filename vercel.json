{
    "version": 2,
    "rewrites": [
        { "source": "/(.*)", "destination": "/src/index.js" }
    ],
    "headers": [
        {
            "source": "/public/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}